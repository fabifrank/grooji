@Library('grooji')
import com.grooji.*;

def execute() {
  node() {

    stage("Send some slack messages") {
      def slack = new com.grooji.SlackNotification(this);
      slack.send('color', 'action', 'appName', 'targetEnv', 'build', 'log', 'recentStage');
      slack.send(slackSend, 'color', 'action', 'appName', 'targetEnv', 'build', 'log');
      slack.send(slackSend, 'color', 'action', 'appName', 'targetEnv', 'build');
      slack.send(slackSend, 'color', 'action', 'appName', 'targetEnv');
      slack.send(slackSend, 'color', 'action', 'appName');
      slack.send(slackSend, slack.COLOR_INFO, slack.BUILD_START, 'appName');
    }
  }
}
